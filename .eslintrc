{        
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "modules": true, 
        "jsx": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "strict": 0,
        "no-underscore-dangle": 0,
        "quotes": [
            2,
            "single"
        ]
    }
}