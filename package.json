  "main": "lib/commonjs/index.js",
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-cli": "6.23.0",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "3.0.0",
    "cross-env": "3.1.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.15.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-prettier": "^2.1.1",
    "fps-measurer": "^0.0.2",
    "postcss-cli": "^2.6.0",
    "prettier": "^1.4.4",
    "react-addons-perf": "^15.4.2",
    "react-resizable": "^1.6.0",
    "react-scripts": "^1.0.7",
    "react-test-renderer": "^15.5.4",
    "reselect-change-memoize": "^1.1.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "object.values": "^1.0.4",
    "react-contextmenu": "^2.6.3",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-redux": "^5.0.4",
    "react-virtualized": "^9.8.0",
    "redux": "^3.6.0",
    "reselect": "^2.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prepare": "rimraf lib && npm run compile",
    "build:test": "REACT_APP_PIVOTGRID_ENV=fps-test npm run build",
    "compile": "npm run lint && npm run compile:js &&npm run compile:css",
    "lint": "eslint src/**/*.js",
    "compile:js": "rimraf lib/commonjs && cross-env NODE_ENV=production babel src/pivotGrid -d lib/commonjs --presets es2015,react-app --ignore test.js",
    "compile:css": "rimraf lib/index.css && postcss --use autoprefixer --use cssnano -o lib/index.css src/pivotGrid/index.css"
  },
  "peerDependencies": {},
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "env": {
      "browser": true,
      "jest": true
    }
  }
}
